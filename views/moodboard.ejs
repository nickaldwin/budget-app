<%- include('partials/header') -%>

<div class="flex flex-col my-5">
  <div class="flex w-screen justify-center"">
    <ul class="flex items-center grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-16 ">
    <% for(var i=0; i<posts.length; i++) {%>
      <li class="w-80 my-5 flex items-center">
        <a href="/post/<%= posts[i]._id%>">
          <img class="" src="<%= posts[i].image%>">
        </a>
      </li>
    <% } %>
    </ul>
  </div>
    <div>
      <div class="m-5 flex justify-center">
        <a class="btn btn-neutral" href="/feed">Return to Inspo Feed</a>
      </div>  
    </div>  
</div>
